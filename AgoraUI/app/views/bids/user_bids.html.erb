<table>
  <thead>
    <tr>
      <th>User Bids</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bids.each do |bid| %>
      <tr>
        <td><%= "Bid Amount: " + bid.bid_amount %></td>
        <td><%= link_to "See Auction", "/auctions/#{bid.auction_id}" %></td>   
      </tr>
    <% end %>
  </tbody>
</table>

<br>